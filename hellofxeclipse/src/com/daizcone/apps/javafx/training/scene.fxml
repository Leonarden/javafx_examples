<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.daizcone.apps.javafx.training.FXMLController">
  
   <top>
      <Label text="JavaFX ROCKS!" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblc1" alignment="CENTER" layoutY="10.0" text="Labelc1" />
            <Label fx:id="lblc2" alignment="CENTER" layoutY="10.0" text="Labelc2" />
            <Label fx:id="lblc3" alignment="CENTER" layoutY="10.0" text="Labelc3" />
         </children>
      </VBox>
   </center>
   <bottom>
      
      <Button fx:id="button1" alignment="CENTER" layoutY="10.0" onAction="#handleButton" text="NumberClicks-0" BorderPane.alignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
   
   </bottom>
  
</BorderPane>
